{
  "editor.codeActionsOnSave": {
      "source.fixAll": true
  },
  "eslint.alwaysShowStatus": true,
  "editor.formatOnSave": true,
  "eslint.debug": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "eslint.trace.server": "verbose",
  "eslint.probe": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "html"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ]
}
